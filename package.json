{
  "name": "reg-log-exp",
  "version": "1.0.0",
  "description": "It is a practice of login, logout using session, cookie, and database.",
  "main": "app.js",
  "scripts": {
    "lint": "eslint src/ --ext .js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build || true && mkdir -p build && cp -r src/* build && babel src --out-dir build",
    "dev": "cd src && NODE_ENV=development nodemon --exec babel-node index.js",
    "lint:fix": "eslint './src/**/*.{js}'",
    "postinstall": "NODE_ENV=production npm run build && node build/index.js"
  },
  "keywords": [
    "express",
    "node",
    "sqlite3",
    "sequelize"
  ],
  "author": "Hooman",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^7.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "object-hash": "^2.0.3",
    "pug": "^3.0.0",
    "sequelize": "^6.3.3",
    "sqlite3": "^5.0.0",
    "sha3": "^2.1.3"
  },
  "engines": {
    "node": "14.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "7.4.2",
    "@babel/register": "7.4.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4"
  }
}
